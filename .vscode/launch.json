{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "fiorilikeapp-config1",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": [
        "--open",
        "index.html"
      ],
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std",
      "env": {
        "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/fiorilikeapp\"}",
        "FIORI_TOOLS_UI5_VERSION": "1.126.2",
        "FIORI_TOOLS_UI5_URI": "https://ui5.sap.com",
        "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/sap\",\"url\":\"https://vhbukse5ci.sap.saprise.corpboots.co.uk:44300/sap/opu/odata/sap/ZJULY_2024_SRV/$metadata\",\"client\":\"https://vhbukse5ci.sap.saprise.corpboots.co.uk:44300/sap/opu/odata/sap/ZJULY_2024_SRV/$metadata\"}]"
      }
    }
  ]
}